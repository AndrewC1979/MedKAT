<?xml version="1.0" encoding="UTF-8"?>
<taeDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>org.ohnlp.medkat.taes.gradeDetector.GradeDetector</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>GradeDetector</name>
    <configurationParameters>
      <configurationParameter>
        <name>PrimarySectionAnnotations</name>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>SecondarySectionAnnotations</name>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>GleasonsGrade</name>
        <description>dict SemClass for Total Gleasons Grade (promary + secondary)</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>PrimaryGleasonsGrade</name>
        <description>dict SemClass for Primary Gleason's Grade</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>SecondaryGleasonsGrade</name>
        <description>dict SemClass for Secondary Gleasons Grade</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>SentenceAnnotationType</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>TokenAnnotationType</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>MaxToLookBeyond</name>
        <description>How many tokens beyond grade term to look for grade level</description>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>ExcludedSubSubsections</name>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>BaseSubSubsectionType</name>
        <description>Base class for subsubsection annotations that are used to subdivide diagnosis sections' subsections</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>BaseGradeSubSubsectionType</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>UnknownMaxValueIndicator</name>
        <description>Value to use of no grade scale defined. Default is "0"</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>PrimarySectionAnnotations</name>
        <value>
          <array>
            <string>org.ohnlp.medkat.taes.sectionFinder.DiagnosisAnnotation</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>GleasonsGrade</name>
        <value>
          <string>GleasonsGrade</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>PrimaryGleasonsGrade</name>
        <value>
          <string>PrimaryGleasonsGrade</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>SecondaryGleasonsGrade</name>
        <value>
          <string>SecondaryGleasonsGrade</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>TokenAnnotationType</name>
        <value>
          <string>uima.tt.TokenAnnotation</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>SentenceAnnotationType</name>
        <value>
          <string>uima.tt.SentenceAnnotation</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>MaxToLookBeyond</name>
        <value>
          <integer>3</integer>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>ExcludedSubSubsections</name>
        <value>
          <array>
            <string>org.ohnlp.medkat.taes.subSubsectionDetector.NotesSubSubsection</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>BaseSubSubsectionType</name>
        <value>
          <string>org.ohnlp.medkat.taes.subSubsectionDetector.DiagnosisBaseSubSubsectionAnnotation</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>BaseGradeSubSubsectionType</name>
        <value>
          <string>org.ohnlp.medkat.taes.subSubsectionDetector.GradeBaseSubSubsectionAnnotation</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>UnknownMaxValueIndicator</name>
        <value>
          <string>Unspecified</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import name="org.ohnlp.medkat.taes.conceptMapper.DictTerm"/>
        <import name="org.ohnlp.medkat.taes.gradeDetector.GradeAnnotation"/>
        <import name="org.ohnlp.medkat.taes.subsectionDetector.SubHeading"/>
      </imports>
    </typeSystemDescription>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">org.ohnlp.medkat.taes.subsectionDetector.SubHeading</type>
          <type allAnnotatorFeatures="true">org.ohnlp.medkat.taes.conceptMapper.DictTerm</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">org.ohnlp.medkat.taes.gradeDetector.GradeAnnotation</type>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
</taeDescription>
