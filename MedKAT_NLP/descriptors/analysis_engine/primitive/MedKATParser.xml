<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>org.ohnlp.medkat.opennlp.annotators.ParserAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>OpenNLPParser</name>
    <description>Parse the document and create phrasal and clausal annotations over the text.  Uses the OpenNLP MaxEnt parser.  

This analysis engine takes a parameter called "ParseTagMapping" which maps each parse tag to a syntax annotation type.  The parse tags come from the standard Penn Tree Bank phrase and clause tags (produced by the OpenNLP parser), and each syntax annotation type must be defined in the type system and have a corresponding JCas Java class.</description>
    <version>1.0</version>
    <vendor>The Apache Software Foundation</vendor>
    <configurationParameters>
      <configurationParameter>
        <name>ModelDirectory</name>
        <description>Directory that contains model files</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>UseTagDictionary</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>CaseSensitiveTagDictionary</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>BeamSize</name>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>AdvancePercentage</name>
        <type>Float</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>ParseTagMappings</name>
        <description>Map between tags and annotation types to be created for the tagged text fragments</description>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>POSTagReplacements</name>
        <description>replacements for POS tags generated by external taggers</description>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>TokenType</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>SentenceType</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>POSFeature</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>ModelDirectory</name>
        <value>
          <string>../OpenNLP/models/english/parser</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>UseTagDictionary</name>
        <value>
          <boolean>false</boolean>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>CaseSensitiveTagDictionary</name>
        <value>
          <boolean>false</boolean>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>ParseTagMappings</name>
        <value>
          <array>
            <string>S,uima.tt.ClauseAnnotation</string>
            <string>SBAR,uima.tt.TCAnnotation</string>
            <string>SBARQ,uima.tt.ClauseAnnotation</string>
            <string>SINV,uima.tt.ClauseAnnotation</string>
            <string>SQ,uima.tt.ClauseAnnotation</string>
            <string>ADJP,uima.tt.AdjPAnnotation</string>
            <string>ADVP,uima.tt.PhraseAnnotation</string>
            <string>CONJP,uima.tt.PhraseAnnotation</string>
            <string>FRAG,uima.tt.PhraseAnnotation</string>
            <string>INTJ,uima.tt.PhraseAnnotation</string>
            <string>LST,uima.tt.NPListAnnotation</string>
            <string>NAC,uima.tt.PhraseAnnotation</string>
            <string>NP,uima.tt.NPAnnotation</string>
            <string>NX,uima.tt.PhraseAnnotation</string>
            <string>PP,uima.tt.PPAnnotation</string>
            <string>PRN,org.ohnlp.medkat.taes.syntacticUnitFinder.SyntacticUnit</string>
            <string>PRT,uima.tt.PhraseAnnotation</string>
            <string>QP,uima.tt.PhraseAnnotation</string>
            <string>RRC,uima.tt.ClauseAnnotation</string>
            <string>UCP,uima.tt.PhraseAnnotation</string>
            <string>VP,uima.tt.VGAnnotation</string>
            <string>WHADJP,uima.tt.AdjPAnnotation</string>
            <string>WHAVP,uima.tt.PhraseAnnotation</string>
            <string>WHNP,uima.tt.NPAnnotation</string>
            <string>WHPP,uima.tt.PPAnnotation</string>
            <string>X,uima.tt.PhraseAnnotation</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>POSTagReplacements</name>
        <value>
          <array>
            <string>CS,before,CC</string>
            <string>CS,if,IN</string>
            <string>CS,when,WRB</string>
            <string>CS,whether,WRB</string>
            <string>CS,,RB</string>
            <string>NP,,NNP</string>
            <string>NPS,,NNPS</string>
            <string>PP,,PRP</string>
            <string>PP$,,PRP$</string>
            <string>AUX,,VB</string>
            <string>AUXD,,VBD</string>
            <string>AUXG,,VBG</string>
            <string>AUXN,,VBN</string>
            <string>AUXP,,VBP</string>
            <string>AUXZ,,VBZ</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>TokenType</name>
        <value>
          <string>uima.tt.TokenAnnotation</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>SentenceType</name>
        <value>
          <string>uima.tt.SentenceAnnotation</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>POSFeature</name>
        <value>
          <string>pennTag</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import location="../../type_system/SPTypes.xml"/>
      </imports>
    </typeSystemDescription>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">uima.tt.TokenAnnotation</type>
          <type allAnnotatorFeatures="true">uima.tt.SentenceAnnotation</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">uima.tt.VGAnnotation</type>
          <type allAnnotatorFeatures="true">uima.tt.TCAnnotation</type>
          <type allAnnotatorFeatures="true">uima.tt.PhraseAnnotation</type>
          <type allAnnotatorFeatures="true">uima.tt.PPAnnotation</type>
          <type allAnnotatorFeatures="true">uima.tt.NPListAnnotation</type>
          <type allAnnotatorFeatures="true">uima.tt.NPAnnotation</type>
          <type allAnnotatorFeatures="true">uima.tt.ClauseAnnotation</type>
          <type allAnnotatorFeatures="true">uima.tt.AdjPAnnotation</type>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
